#include<iostream>
using namespace std;
int main(){
	int n;
	int arr[n];
	cout<<"enter the size of array";
	cin>>n;
	cout<<"enter the elements";
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	int key;
	cout<<"enter the element to search";
	cin>>key;
	for(int i=0;i<n;i++){
		arr[i]=key;
		return i;
	}
	
	return 0;
}
